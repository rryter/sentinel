{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../../dist/out-tsc/api",
    "declaration": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": true
  },
  "include": ["generated/**/*.ts"],
  "exclude": ["generated/**/*.spec.ts"]
}
